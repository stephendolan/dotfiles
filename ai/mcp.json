{
  "mcpServers": {
    "betterstack": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.betterstack.com",
        "--header",
        "Authorization: Bearer ${BETTERSTACK_API_TOKEN}"
      ],
      "env": {
        "BETTERSTACK_API_TOKEN": "${BETTERSTACK_API_TOKEN}"
      }
    },
    "chartmogul": {
      "command": "chartmogul",
      "args": ["mcp"]
    },
    "helpscout": {
      "command": "helpscout",
      "args": ["mcp"]
    },
    "omnifocus": {
      "command": "of",
      "args": ["mcp"]
    },
    "superset": {
      "type": "http",
      "url": "https://api.superset.sh/api/agent/mcp"
    },
    "ynab": {
      "command": "ynab",
      "args": ["mcp"]
    }
  }
}
